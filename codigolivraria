select * from livros;

select livro,editora from livros;

select livro,UFeditora from livros
where sexo = 'M';

select livro,numeropaginas from livros
where sexo = 'M';

select preco from livros
where UFeditora = 'SP';

select * from livros
where sexo = 'M' and (UFeditora = 'SP' or UFeditora = 'RJ');
