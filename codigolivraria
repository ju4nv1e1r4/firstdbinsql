create database livraria;

create table livros (
	livro varchar (30),
	autor varchar (30),
	sexo char (1),
	numeropaginas int (3),
	editora varchar (20),
	preco float (6,2),
	UFeditora char (2),
	anopublicacao int (4)
	);

